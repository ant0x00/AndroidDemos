<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/rl_frame"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@color/color_ffffff"
    android:orientation="vertical">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">
        <RelativeLayout
            android:id="@+id/rl_tab"
            android:layout_width="match_parent"
            android:layout_height="@dimen/size_50dp"
            android:visibility="gone">
            <LinearLayout
                android:id="@+id/ll_first_date"
                android:layout_width="@dimen/size_150dp"
                android:layout_height="@dimen/size_48dp"
                android:layout_centerVertical="true"
                android:layout_alignParentLeft="true"
                android:gravity="center"
                android:layout_marginLeft="@dimen/size_15dp"
                android:orientation="vertical">
                <TextView
                    android:id="@+id/tv_first_date_title"
                    android:layout_width="@dimen/size_150dp"
                    android:gravity="center"
                    android:layout_height="wrap_content"
                    android:textColor="@color/color_ff5523"
                    android:textSize="@dimen/size_19dp"/>
                <TextView
                    android:id="@+id/tv_first_date"
                    android:layout_width="@dimen/size_150dp"
                    android:gravity="center"
                    android:layout_height="wrap_content"
                    android:textColor="@color/color_999999"
                    android:textSize="@dimen/size_14dp"
                    android:visibility="gone"/>

            </LinearLayout>
            <LinearLayout
                android:id="@+id/ll_second_date"
                android:layout_width="@dimen/size_150dp"
                android:layout_height="@dimen/size_48dp"
                android:layout_marginRight="@dimen/size_15dp"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:gravity="center"
                android:orientation="vertical">
                <TextView
                    android:id="@+id/tv_second_date_title"
                    android:layout_width="@dimen/size_150dp"
                    android:gravity="center"
                    android:layout_height="wrap_content"

                    android:textColor="@color/color_ff5523"
                    android:textSize="@dimen/size_19dp"/>
                <TextView
                    android:id="@+id/tv_second_date"
                    android:layout_width="@dimen/size_150dp"
                    android:gravity="center"
                    android:layout_height="wrap_content"
                    android:textColor="@color/color_999999"
                    android:textSize="@dimen/size_14dp"
                    android:visibility="gone"/>
            </LinearLayout>
            <RelativeLayout
                android:id="@+id/rl_selected_line_frame"
                android:layout_width="match_parent"
                android:layout_height="@dimen/size_2dp"
                android:layout_below="@id/ll_first_date"
                android:layout_marginRight="@dimen/size_15dp"
                android:layout_marginLeft="@dimen/size_15dp">
                <View
                    android:id="@+id/selected_line"

                    android:layout_width="@dimen/size_150dp"
                    android:layout_height="@dimen/size_2dp"
                    android:background="@color/color_ff5523"></View>

            </RelativeLayout>


        </RelativeLayout>


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/size_30dp"
            android:background="@color/color_f5f5f5"
            android:orientation="horizontal">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/size_30dp"
                android:background="@color/color_f5f5f5"
                android:layout_marginLeft="@dimen/size_5dp"
                android:layout_marginRight="@dimen/size_5dp"
                android:orientation="horizontal">
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:gravity="center"
                    android:textColor="@color/color_ff5523"
                    android:textSize="@dimen/size_12dp"
                    android:layout_weight="2"
                    android:text="@string/sun"/>
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:gravity="center"
                    android:textColor="@color/color_333333"
                    android:textSize="@dimen/size_12dp"
                    android:layout_weight="2"
                    android:text="@string/num_one"/>
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:gravity="center"
                    android:textColor="@color/color_333333"
                    android:textSize="@dimen/size_12dp"
                    android:layout_weight="2"
                    android:text="@string/num_two"/>
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:gravity="center"
                    android:textColor="@color/color_333333"
                    android:textSize="@dimen/size_12dp"
                    android:layout_weight="2"
                    android:text="@string/num_three"/>
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:gravity="center"
                    android:textColor="@color/color_333333"
                    android:textSize="@dimen/size_12dp"
                    android:layout_weight="2"
                    android:text="@string/num_four"/>
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:gravity="center"
                    android:textColor="@color/color_333333"
                    android:textSize="@dimen/size_12dp"
                    android:layout_weight="2"
                    android:text="@string/num_five"/>
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:gravity="center"
                    android:textColor="@color/color_ff5523"
                    android:textSize="@dimen/size_12dp"
                    android:layout_weight="2"
                    android:text="@string/num_six"/>
                </LinearLayout>
        </LinearLayout>
        <ScrollView
            android:id="@+id/sv_frame"
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <LinearLayout
                android:id="@+id/date_frame"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical"
                android:layout_marginLeft="@dimen/size_5dp"
                android:layout_marginRight="@dimen/size_5dp">


            </LinearLayout>
        </ScrollView>
    </LinearLayout>
    <TextView
        android:id="@+id/move_date"
        android:layout_width="@dimen/size_150dp"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:textColor="@color/color_999999"
        android:textSize="@dimen/size_14dp"
        android:visibility="gone"/>
</RelativeLayout>
